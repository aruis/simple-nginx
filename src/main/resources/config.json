{
  "port": 9000,
  "resource": [
    {
      "prefix": "/static1",
      "dir": "static1",
      "reroute404": "/static1",
      "cachingEnabled": true,
      "maxAgeSeconds": 30
    },
    {
      "prefix": "/static2",
      "dir": "static2",
      "reroute404": "/static2/404.html",
      "cachingEnabled": false
    }
  ],
  "upstream": [
    {
      "prefix": "/a",
      "url": "http://127.0.0.1:8080"
    },
    {
      "prefix": "/b",
      "url": "http://127.0.0.1:8888"
    }
  ]
}
